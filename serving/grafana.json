{
    "annotations": {
        "list": [
            {
                "builtIn": 1,
                "datasource": "-- Grafana --",
                "enable": true,
                "hide": true,
                "iconColor": "rgba(0, 211, 255, 1)",
                "name": "Annotations & Alerts",
                "target": {
                    "limit": 100,
                    "matchAny": false,
                    "tags": [],
                    "type": "dashboard"
                },
                "type": "dashboard"
            }
        ]
    },
    "editable": true,
    "graphTooltip": 0,
    "id": null,
    "links": [],
    "panels": [
        {
            "title": "System Latency (Avg)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 0
            },
            "targets": [
                {
                    "expr": "rate(http_request_duration_seconds_sum[1m]) / rate(http_request_duration_seconds_count[1m])",
                    "legendFormat": "Avg Latency (s)",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s",
                    "color": {
                        "mode": "palette-classic"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "red",
                                "value": 0.150
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "Request Throughput",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 0
            },
            "targets": [
                {
                    "expr": "rate(http_requests_total[1m])",
                    "legendFormat": "Req/sec",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "rps"
                }
            }
        },
        {
            "title": "Model Output Distribution",
            "type": "heatmap",
            "gridPos": {
                "h": 10,
                "w": 12,
                "x": 0,
                "y": 8
            },
            "targets": [
                {
                    "expr": "sum(rate(model_prediction_minutes_bucket[5m])) by (le)",
                    "format": "heatmap",
                    "refId": "A"
                }
            ],
            "options": {
                "calculate": false,
                "color": {
                    "mode": "scheme",
                    "scheme": "Spectral",
                    "exponent": 0.5
                },
                "yAxis": {
                    "axisPlacement": "left"
                }
            },
            "description": "Visualizes the distribution of model output. Heavy Distibution shift may signal a heavy behavior change."
        },
        {
            "title": "Input Data Drift (Distance Feature)",
            "type": "timeseries",
            "gridPos": {
                "h": 10,
                "w": 12,
                "x": 12,
                "y": 8
            },
            "targets": [
                {
                    "expr": "rate(input_feature_distance_sum[5m]) / rate(input_feature_distance_count[5m])",
                    "legendFormat": "Avg Input Distance",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "palette-classic"
                    },
                    "custom": {
                        "axisLabel": "Distance"
                    }
                }
            },
            "description": "Tracks the average distance in incoming requests. A spike may indicate the input data is drifting from the training set."
        }
    ],
    "refresh": "5s",
    "schemaVersion": 36,
    "style": "dark",
    "tags": [
        "mlops",
        "serving"
    ],
    "templating": {
        "list": []
    },
    "time": {
        "from": "now-15m",
        "to": "now"
    },
    "title": "Taxi NYC Model Monitoring Dashboard",
    "uid": "model_monitor_v1",
    "version": 1,
    "weekStart": ""
}